void updateEnemyBullet(BULLET *b) {
    if(b->active) {
        if(!collision(player.col, player.row, player.width, player.height, b->col, b->row, b->width, b->height) && b->col + b->width > 0) {
            b->col -= b->bulletSpeed;
        } else {
            if(collision(player.col, player.row, player.width, player.height, b->col, b->row, b->width, b->height)) {
                if(!sameLane(player.row, b->row)) {
                    b->col -= b->bulletSpeed;
                } else {
                    if(!player.buffed && !player.tempInvincible) {
                        b->active = 0;
                        lives--;
                        tempLives = lives;
                        player.tempInvincible = 1;
                        invulnerabilityTimer = 100;
                        player.aniState = 24;
                        player.curFrame = 3;
                        stopSoundB();
                        playSoundB(hitSfx_data, hitSfx_length, 0);
                    } else if (player.buffed){
                        b->active = 0;
                        player.buffed = 0;
                    } else {
                        b->col -= b->bulletSpeed;
                    }
                }
            } else {
                b->active = 0;
            }
        }
    }
}

